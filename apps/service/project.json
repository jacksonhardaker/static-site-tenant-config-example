{
  "name": "service",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/service",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/service"
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies",
          "params": "forward"
        }
      ],
      "configurations": {
        "development": {
          "outputPath": "apps/service"
        },
        "development:orange": {
          "outputPath": "apps/service"
        },
        "development:apple": {
          "outputPath": "apps/service"
        },
        "production": {},
        "production:orange": {},
        "production:apple": {}
      }
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "service:build",
        "dev": true
      },
      "dependsOn": [
        {
          "target": "build",
          "projects": "dependencies",
          "params": "forward"
        }
      ],
      "configurations": {
        "development": {
          "buildTarget": "service:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "service:build:production",
          "dev": false
        },
        "development:orange": {
          "buildTarget": "service:build:development:orange",
          "dev": true
        },
        "development:apple": {
          "buildTarget": "service:build:development:apple",
          "dev": true
        }
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "service:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/service/jest.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"]
    }
  },
  "tags": [],
  "implicitDependencies": ["config"]
}
